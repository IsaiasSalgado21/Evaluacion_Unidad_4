RewriteRule ^home/?$ views/home.php 
RewriteRule ^profile/?$ views/account-profile.php

RewriteRule ^user-list/?$ views/users/user-list.php 
RewriteRule ^user-details/?$ views/users/user-details.php 

RewriteRule ^customers-list/?$ views/customers/customers-list.php 
RewriteRule ^customer-details/?$ views/customers/customer-details.php 

